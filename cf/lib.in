# PLplot build rule for library under Unix.
# -----------------------------------------------------------------------

PLLIB		= $(PLLIB_PATH)libplplot$(LIB_TAG).a

libs:	$(PLLIB)

LIB_OBJS = $(OBJ) $(TCL_OBJ) $(TK_OBJ) $(DRIVERS_OBJ) $(FSTUB_OBJ)

$(PLLIB):	$(LIB_OBJS)
	-rm $(PLLIB)
	$(LIB_BUILD) $(PLLIB) $(LIB_OBJS)
	$(RANLIB) $(PLLIB)

