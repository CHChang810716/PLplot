# Cleanup

clean:
	-rm * shared/* ../lib/libpl*

# Autogeneration of dependencies.
# I use gcc -MM because otherwise sytem headers get included, which sucks.

depend:
	gcc -MM $(CORE_C) $(DRIVERS_C) $(UTIL_C) $(FSTUB_C) $(TCL_C) $(TK_C) \
		> deps.in
	@echo "Rerun configure for the new dependencies to take effect"

