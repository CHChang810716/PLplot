all: c/x01c@EXEEXT@
c/x01c@EXEEXT@:
	cd c; $(MAKE)
c_clean:
	cd c; $(MAKE) clean
.PHONY: c_clean
clean: c_clean
x01c.psc: c/x01c@EXEEXT@
	echo C front-end for psc
	./plplot-test.sh --verbose --front-end=c --device=psc
test: x01c.psc
@COMPARE_COMMENT@compare: x01c.psc

