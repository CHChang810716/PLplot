DC = @DC@
EXEEXT = @EXEEXT@
D_INSTDIR = @D_INSTDIR@
INSTDIR = @INSTDIR@

EXECUTABLES_list = \
		 x03d$(EXEEXT) \
		 x04d$(EXEEXT) \
		 x05d$(EXEEXT) \
		 x06d$(EXEEXT) \
		 x07d$(EXEEXT) \
		 x08d$(EXEEXT) \
		 x10d$(EXEEXT) \
		 x12d$(EXEEXT) \
		 x13d$(EXEEXT) \

all: $(EXECUTABLES_list)

clean:
	rm -f $(EXECUTABLES_list)

.d$(EXEEXT):
$(DC) $< -o $@ -I$(D_INSTDIR) $(INSTDIR)/libplplotd.so

.SUFFIXES: .d $(EXEEXT)
